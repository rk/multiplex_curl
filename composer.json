{
  "name": "rk/multiplex_curl",
  "description": "A curl library with built-in multiplex support.",
  "type": "library",
  "keywords": [
    "curl"
  ],
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "rk",
      "email": "robert@enforme.com"
    }
  ],
  "require": {
    "php": "^7.4 || >=8.0",
    "ext-mbstring": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "RK\\MultiplexCurl\\": "src/"
    },
    "exclude-from-classmap": [
      "tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "RK\\MultiplexCurl\\Tests\\": "tests/"
    }
  }
}